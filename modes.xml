      </program>
      <program name="cg-proc -w">
        <file name="tur-uzb.rlx.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="tur-uzb.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="tur-uzb.autobil.bin"/>
      </program>
      <program name="lrx-proc">
        <file name="tur-uzb.autolex.bin"/>
      </program>
      <program name="apertium-transfer -b">
        <file name="apertium-tur-uzb.tur-uzb.t1x"/>
        <file name="tur-uzb.t1x.bin"/>
      </program>
      <program name="hfst-proc $1">
        <file name="tur-uzb.autogen.hfst"/>
      </program>
    </pipeline>
  </mode>

